{
    "title": "我的一本书",
    "author" : "yu",
    "description" : "我第一本书的描述，很好",
    "language" : "zh-hans",
    "plugins": [
        "-lunr",
        "-search",
        "chapter-fold",
        "splitter",
        "page-treeview",
        "page-toc-button",
        "sectionx",
        "hide-element",
        "github",
        "simple-page-toc",
        "accordion"
    ],
    "pluginsConfig": {
        "pluginsConfig": {
            "simple-page-toc": {
                "maxDepth": 3,
                "skipFirstH1": true
            }
        },
        "github": {
            "url": "https://github.com/yulilong/book"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "anchor-navigation-ex": {
            "isShowTocTitleIcon": true 
        },
        "sectionx": {
            "tag": "b"
        }
    },
    "links" : {
        "sidebar" : {
            "个性链接1" : "https://www.baidu.com"
        }
    }
}